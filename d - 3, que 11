#include <stdio.h>

struct student {
    char name[50];
    int id;
    float gpa;
};

int main() {
    // create a student object
    struct student s1;
    
    // initialize the fields
    printf("Enter student name: ");
    scanf("%s", s1.name);
    printf("Enter student ID: ");
    scanf("%d", &s1.id);
    printf("Enter student GPA: ");
    scanf("%f", &s1.gpa);
    
    // print the values of the fields
    printf("Name: %s\n", s1.name);
    printf("ID: %d\n", s1.id);
    printf("GPA: %f\n", s1.gpa);
    
    return 0;
}
